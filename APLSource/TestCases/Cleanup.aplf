 Cleanup;⎕USING;rk;rc;msg
 rk←GetRegKey,∆Log
 :If WinSys.IsRunningAsAdmin
 :AndIf 0≠rc←WinReg.DeleteSubKey rk
     msg←''
     msg,←⊂'>>> Delete this key:'
     msg,←⊂'    ',rk
     msg,←⊂'    from the Windows Registry failed. Please delete with RegEdit: otherwise the next test run will fail!'
     CommTools.Pause msg
 :EndIf
